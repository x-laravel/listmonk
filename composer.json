{
  "name": "x-laravel/listmonk",
  "description": "Laravel integration for Listmonk - Self-hosted newsletter and mailing list manager",
  "keywords": [
    "laravel",
    "listmonk",
    "newsletter",
    "email",
    "mailing-list",
    "subscription"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mehmet ÖĞMEN",
      "email": "www@mehmetogmen.com.tr",
      "homepage": "https://www.mehmetogmen.com.tr",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/http": "^10.0|^11.0|^12.0",
    "illuminate/queue": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^10.0|^11.0",
    "mockery/mockery": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "XLaravel\\Listmonk\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "XLaravel\\Listmonk\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "XLaravel\\Listmonk\\ListmonkServiceProvider"
      ],
      "aliases": {
        "Listmonk": "XLaravel\\Listmonk\\Facades\\Listmonk"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable"
}
